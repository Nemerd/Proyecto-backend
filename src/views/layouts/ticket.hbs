<table id="lista-de-productos" class="productos">
    {{#each products}}
    <tr>
        <td>{{product.code}}</td>
        <td>{{product.title}}</td>
        <td>{{product.description}}</td>
        <td>{{product.price}}</td>
        <td><img src={{product.thumbnail}}></td>
        <td>Quantity: {{quantity}}</td>
    </tr>
    {{/each}}
</table>

<p>Total: ${{amount}}</p>
<p>{{purchaser}}</p>
<p>{{code}}</p>