<form action="/" method="post" id="changer">
    <label for="title">Title:</label>
    <input type="text" placeholder="Title" name="title">
    <br>

    <label for="code">Code:</label>
    <input type="text" placeholder="123456" name="code">
    <br>

    <label for="description">Description:</label>
    <input type="text" placeholder="Description" name="description">
    <br>

    <label for="price">Price:</label>
    <input type="text" placeholder="Price" name="price">
    <br>

    <label for="stock">Stock:</label>
    <input type="text" placeholder="10" name="stock">
    <br>

    <label for="thumbnail">Thumbnail:</label>
    <input type="text" placeholder="Thumbnail" name="thumbnail">
    <br>

    <button type="submit">Subir</button>
</form>
<script src="/public/productChanger.js"></script>